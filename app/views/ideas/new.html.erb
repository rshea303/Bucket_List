<!DOCTYPE html>
<html>
  <body>
    <h1>Please enter your new idea</h1>
    <%= form_for @idea do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :description %>
      <%= f.text_area :description %>
      <%= f.submit 'Submit' %>
    <% end %>
  </body>
</html>

